{
  "manifest_version": 3,
  "name": "My Log Extension",
  "version": "1.0",
  "description": "A simple extension for logging data.",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "http://127.0.0.1:5001/*"
  ]
}